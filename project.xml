                        stats.vipInfo.initialLevel = data.vipInfo.level || 1
                        stats.vipInfo.initialXp = data.vipInfo.xp || 0
                    } else {
                        stats.vipInfo.finalLevel = data.vipInfo.level || 1
                        stats.vipInfo.finalXp = data.vipInfo.xp || 0
