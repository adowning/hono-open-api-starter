        ),
        [HttpStatusCodes.BAD_REQUEST]: jsonContent(
            badRequestSchema,
            'Bad Request',
        ),
        // [HttpStatusCodes.UNPROCESSABLE_ENTITY]: jsonContent(
        //     createErrorSchema(insertUserSchema),
        //     'The validation error(s)',
        // ),
    },
})

export const sessionRoute = createRoute({
    method: 'get',
    path: '/auth/me',
    tags,
