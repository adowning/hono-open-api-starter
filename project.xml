        if (url.pathname.includes('ws/proxy')) {
            console.log(url.searchParams.get('data'))
        }

        if (url.pathname.includes('EjsFrontWeb/fs')) {
            console.log('here')
            // const whatWeNeed = await fetch('https://dev.cashflowcasino.com/EjsFrontWeb/fs', {
            //     headers: {
