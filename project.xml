    close(ws: ServerWebSocket<WebSocketData>) {
        const { user } = ws.data
        console.log(`User ${user.username} left the chat`)
        ws.unsubscribe('chat')
        ws.publish('chat', `${user.username} has left the chat.`)
    },
}
