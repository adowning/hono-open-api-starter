//   const data = event.data;
//   if (data && data.type === 'GAME_EVENT_BALANCE_UPDATE') {
//     authStore.updateBalance(data.payload.balance); // Example
//   } else if (data === 'CLOSE_GAME_REQUEST') {
//     // Handle game close request, navigate to lobby
//   }
// };
</script>

